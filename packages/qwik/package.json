{
  "name": "@modular-forms/qwik",
  "description": "The modular and type-safe form library for Qwik",
  "version": "0.1.1",
  "license": "MIT",
  "author": "Fabian Hiller",
  "homepage": "https://modularforms.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabian-hiller/modular-forms"
  },
  "keywords": [
    "modular-forms",
    "form",
    "forms",
    "qwik",
    "qwik-city",
    "performance",
    "bundle-size",
    "form-validation",
    "validation",
    "typescript",
    "type-safe"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.qwik.mjs",
  "qwik": "./dist/index.qwik.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.qwik.mjs",
      "require": "./dist/index.qwik.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build --mode lib && tsc",
    "format": "prettier --write .",
    "format.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.23.0",
    "@builder.io/qwik-city": "^0.6.5",
    "@types/eslint": "^8.21.2",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-qwik": "^0.23.0",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "typescript": "4.9.5",
    "undici": "^5.21.0",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^0.23.0",
    "@builder.io/qwik-city": "^0.6.5"
  }
}
