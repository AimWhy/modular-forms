{
  "name": "@modular-forms/solid",
  "description": "The modular form library for SolidJS",
  "version": "0.8.0",
  "license": "MIT",
  "author": "Fabian Hiller",
  "homepage": "https://modularforms.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabian-hiller/modular-forms"
  },
  "keywords": [
    "modular-forms",
    "form",
    "forms",
    "solid",
    "solidjs",
    "performance",
    "bundle-size",
    "reactive",
    "form-validation",
    "validation",
    "typescript"
  ],
  "sideEffects": false,
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.jsx",
  "main": "./dist/index.cjs.jsx",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.esm.jsx",
      "require": "./dist/index.cjs.jsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "npx tsc --noEmit",
    "publish": "npm publish"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "solid-js": "^1.6.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}
